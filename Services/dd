        <script>
            $(document).ready(function () {
                /* Product Zoom */
                function product_zoom() {
                    $(".zoompro").elevateZoom({
                        gallery: "gallery",
                        galleryActiveClass: "active",
                        zoomWindowWidth: 300,
                        zoomWindowHeight: 100,
                        scrollZoom: false,
                        zoomType: "inner",
                        cursor: "crosshair"
                    });
                }
                product_zoom();
            });
        </script>

